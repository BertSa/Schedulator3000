{
  "env": {
    "es2021": true,
    "browser": true,
    "jest": true
  },
  "extends": [
    "airbnb-typescript",
    "airbnb"
  ],
  "parserOptions": {
    "project": "./tsconfig.json",
    "ecmaVersion": "latest",
    "sourceType": "script"
  },
  "rules": {
    "accessor-pairs": 2,
    "array-bracket-newline": 2,
    "arrow-parens": 2,
    "comma-spacing": 2,
    "comma-style": 2,
    "constructor-super": 2,
    "for-direction": 2,
    "function-paren-newline": 0,
    "getter-return": 2,
    "implicit-arrow-linebreak": 0,
    "import/extensions": [
      "error",
      "ignorePackages",
      {
        "js": "never",
        "jsx": "never",
        "ts": "never",
        "tsx": "never"
      }
    ],
    "import/no-cycle": 0,
    "jsx-quotes": 2,
    "max-len": [
      "error",
      {
        "code": 140,
        "ignoreComments": true,
        "ignoreRegExpLiterals": true,
        "ignoreStrings": true,
        "ignoreTemplateLiterals": true,
        "ignoreUrls": true,
        "tabWidth": 2
      }
    ],
    "no-async-promise-executor": 0,
    "no-case-declarations": 2,
    "no-class-assign": 2,
    "no-compare-neg-zero": 2,
    "no-cond-assign": 2,
    "no-confusing-arrow": 0,
    "no-console": 0,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-else-if": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-pattern": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-import-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-loss-of-precision": 2,
    "no-misleading-character-class": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-new-symbol": 2,
    "no-nonoctal-decimal-escape": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-plusplus": 0,
    "no-prototype-builtins": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-self-assign": 2,
    "no-setter-return": 2,
    "no-shadow-restricted-names": 2,
    "no-sparse-arrays": 2,
    "no-this-before-super": 2,
    "no-undef": 2,
    "no-underscore-dangle": 0,
    "no-unexpected-multiline": 2,
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unsafe-optional-chaining": 2,
    "no-unused-labels": 2,
    "no-unused-vars": 2,
    "no-useless-backreference": 2,
    "no-useless-catch": 2,
    "no-useless-escape": 2,
    "no-with": 2,
    "object-curly-newline": 0,
    "prefer-const": 2,
    "prefer-destructuring": 1,
    "prefer-exponentiation-operator": 2,
    "prefer-template": 1,
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    ],
    "react/jsx-props-no-spreading": 0,
    "react/no-array-index-key": 1,
    "react/no-unstable-nested-components": 0,
    "react/require-default-props": 1,
    "require-yield": 2,
    "use-isnan": 2,
    "valid-typeof": 2
  }
}
